{
  "name": "b402-developer-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for building paid APIs using the b402 payment protocol on BNB Chain",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc && node dist/server.js",
    "test": "npm run build && node dist/testClient.js",
    "test:shell": "bash test-request.sh",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "b402",
    "payment-api",
    "payments",
    "usdt",
    "usdc",
    "usd1",
    "bnb-chain",
    "bsc"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "b402-express": "file:../b402-sdk-repo/packages/express",
    "b402-sdk": "latest",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "openai": "^4.78.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.2",
    "typescript": "^5.9.3"
  }
}
