# Server Configuration
PORT=3000

# Payment Configuration
# The wallet address that will receive payments via the facilitator
PAY_TO_ADDRESS=0xYourWalletAddress

# Network Configuration
# Options: "mainnet" or "testnet"
NETWORK=testnet

# Payment Settings
# Price per request (in USD)
PRICE=0.01
# Token to accept: "USD1", "USDT", or "USDC"
TOKEN=USD1

# OpenAI Configuration
# Your OpenAI API key for AI processing
OPENAI_API_KEY=your_openai_api_key_here
# Optional: override the OpenAI base URL (advanced)
# OPENAI_BASE_URL=https://api.openai.com/v1

# AI Provider Configuration
# Options: "openai" (default) or "eigenai"
# AI_PROVIDER=openai
# AI_MODEL=gpt-4o-mini
# AI_TEMPERATURE=0.7
# AI_MAX_TOKENS=500
# AI_SEED=42

# Test Client Configuration (optional - only needed for testing with payments)
# CLIENT_PRIVATE_KEY=your_test_wallet_private_key_here
# AGENT_URL=http://localhost:3000

# Optional: Facilitator configuration
# FACILITATOR_URL=https://your-custom-facilitator.com

# Optional: EigenAI configuration (required if AI_PROVIDER=eigenai)
# EIGENAI_API_KEY=your_eigenai_api_key_here
# EIGENAI_BASE_URL=https://eigenai.eigencloud.xyz/v1

# Optional: Debug logging
B402_DEBUG=true
